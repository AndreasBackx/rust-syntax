{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "convert YAML to JSON",
      "type": "shell",
      "command": "npx",
      "args": [
        "js-yaml",
        "syntaxes/rust.tmLanguage.yaml",
        ">",
        "syntaxes/rust.tmLanguage.json"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "presentation": {
        "clear": false,
        "echo": true,
        "focus": false,
        "panel": "shared",
        "reveal": "silent",
        "showReuseMessage": true
      }
    }
  ]
}
